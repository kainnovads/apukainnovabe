# App
APP_NAME=
PORT=3333
HOST=localhost
NODE_ENV=development

# API URL buat Nuxt akses (kalau pake NUXT_PUBLIC_*)
NUXT_PUBLIC_API_BASE=
NUXT_PUBLIC_AUTH_BASE=

LOG_LEVEL=info

# App Key (wajib, auto-generate via node ace generate:key)
APP_KEY=

# Database
DB_CONNECTION=
DB_HOST=127.0.0.1
DB_PORT=5432
DB_SSL=
DB_USER=root
DB_PASSWORD=root
DB_DATABASE=app

# Auth
DRIVE_DISK=
SESSION_DRIVER=cookie

# Mail (optional, kalau ada fitur kirim email)
SMTP_HOST=
SMTP_PORT=
SMTP_USERNAME=
SMTP_PASSWORD=
MAIL_FROM_ADDRESS=
SMTP_FROM_NAME=

# CORS
CORS_ENABLED=

# Cache (kalau pake redis atau lainnya, optional)
CACHE_VIEWS=
MAILGUN_API_KEY=
MAILGUN_DOMAIN=
SPARKPOST_API_KEY=